{
  "strictNullChecks": true,
  "allowUnreachableCode": true,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "moduleResolution": "node",
    "baseUrl": "",
    "module": "commonjs",
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
    ]
  },
  "exclude": [
    "node_modules/@types/node",
    "node_modules",
    "platforms",
    "**/main.web.ts",
    "**/*.aot.ts"
  ]
}